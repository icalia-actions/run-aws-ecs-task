    "@types/node": "^14.14.37",
    "@types/tmp": "^0.2.0",
    "@vercel/ncc": "^0.27.0",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "jest-runtime": "^26.6.3",
    "prettier": "^2.2.1",
